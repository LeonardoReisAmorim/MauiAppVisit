<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiAppVisit.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:MauiAppVisit.View"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        Route="Login" ContentTemplate="{DataTemplate views:Login}" />
    
    <ShellContent
        Route="Register" ContentTemplate="{DataTemplate views:Register}" />

    <ShellContent
       Route="Locations" ContentTemplate="{DataTemplate views:Locations}" />

    

</Shell>
